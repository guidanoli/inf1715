(program
	(def_function id="conds"
		(def_variable id="a"
			(reference id="int" kind="type" line=0)
		)
		(def_variable id="b"
			(reference id="int" kind="type" line=0)
		)
		(block
			(statement if
				(condition ==
					(expression variable
						(variable id
							(reference id="a" kind="variable" line=1)
						)
					)
					(expression variable
						(variable id
							(reference id="b" kind="variable" line=1)
						)
					)
				)
				(block)
			)
			(statement if
				(condition ~=
					(expression variable
						(variable id
							(reference id="a" kind="variable" line=1)
						)
					)
					(expression variable
						(variable id
							(reference id="b" kind="variable" line=1)
						)
					)
				)
				(block)
			)
			(statement if
				(condition <=
					(expression variable
						(variable id
							(reference id="a" kind="variable" line=1)
						)
					)
					(expression variable
						(variable id
							(reference id="b" kind="variable" line=1)
						)
					)
				)
				(block)
			)
			(statement if
				(condition >=
					(expression variable
						(variable id
							(reference id="a" kind="variable" line=1)
						)
					)
					(expression variable
						(variable id
							(reference id="b" kind="variable" line=1)
						)
					)
				)
				(block)
			)
			(statement if
				(condition <
					(expression variable
						(variable id
							(reference id="a" kind="variable" line=1)
						)
					)
					(expression variable
						(variable id
							(reference id="b" kind="variable" line=1)
						)
					)
				)
				(block)
			)
			(statement if
				(condition >
					(expression variable
						(variable id
							(reference id="a" kind="variable" line=1)
						)
					)
					(expression variable
						(variable id
							(reference id="b" kind="variable" line=1)
						)
					)
				)
				(block)
			)
		)
	)
)
