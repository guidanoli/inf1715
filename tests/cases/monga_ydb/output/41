(program
	(def_variable id="a"
		(reference id="int" kind="type" line=0)
	)
	(def_function id="f"
		(parameter id="a"
			(reference id="float" kind="type" line=0)
		)
		(block
			(statement print
				(expression variable
					(variable id
						(reference id="a" kind="parameter" line=2)
					)
				)
			)
			(statement if
				(condition >
					(expression variable
						(variable id
							(reference id="a" kind="parameter" line=2)
						)
					)
					(expression real "0")
				)
				(block
					(def_variable id="a"
						(reference id="int" kind="type" line=0)
					)
					(statement print
						(expression variable
							(variable id
								(reference id="a" kind="variable" line=5)
							)
						)
					)
				)
			)
		)
	)
)
