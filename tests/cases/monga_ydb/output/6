(program
	(def_function id="print_int"
		(parameter id="x" type="int")
		(block
			(statement print
				(expression variable
					(variable "x")
				)
			)
		)
	)
	(def_function id="dec" type="int"
		(parameter id="x" type="int")
		(block
			(statement return
				(expression -
					(expression variable
						(variable "x")
					)
					(expression integer "1")
				)
			)
		)
	)
	(def_function id="fac" type="int"
		(parameter id="n" type="int")
		(block
			(def_variable id="prod" type="int")
			(statement =
				(variable "prod")
				(expression integer "1")
			)
			(statement while
				(condition >
					(expression variable
						(variable "n")
					)
					(expression integer "0")
				)
				(block
					(statement =
						(variable "prod")
						(expression *
							(expression variable
								(variable "prod")
							)
							(expression variable
								(variable "n")
							)
						)
					)
					(statement call
						(call function="print_int"
							(expression variable
								(variable "prod")
							)
						)
					)
					(statement =
						(variable "n")
						(expression call
							(call function="dec"
								(expression variable
									(variable "n")
								)
							)
						)
					)
				)
			)
			(statement return
				(expression variable
					(variable "prod")
				)
			)
		)
	)
)
