(program
	(def_type id="a"
		(typedesc "int")
	)
	(def_type id="b"
		(typedesc array
			(typedesc "int")
		)
	)
	(def_type id="c"
		(typedesc array
			(typedesc array
				(typedesc "int")
			)
		)
	)
	(def_type id="d"
		(typedesc array
			(typedesc record
				(field id="x" type="int")
				(field id="y" type="float")
			)
		)
	)
	(def_type id="e"
		(typedesc array
			(typedesc array
				(typedesc record
					(field id="x" type="int")
					(field id="y" type="float")
				)
			)
		)
	)
	(def_type id="f"
		(typedesc record
			(field id="x" type="a")
			(field id="y" type="b")
		)
	)
)
