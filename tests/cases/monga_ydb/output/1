(program
	(def_type id="a"
		(typedesc id
			(reference id="int" kind="type" line=0)
		)
	)
	(def_type id="b"
		(typedesc array
			(typedesc id
				(reference id="int" kind="type" line=0)
			)
		)
	)
	(def_type id="c"
		(typedesc array
			(typedesc array
				(typedesc id
					(reference id="int" kind="type" line=0)
				)
			)
		)
	)
	(def_type id="d"
		(typedesc array
			(typedesc record
				(field id="x"
					(reference id="int" kind="type" line=0)
				)
				(field id="y"
					(reference id="float" kind="type" line=0)
				)
			)
		)
	)
	(def_type id="e"
		(typedesc array
			(typedesc array
				(typedesc record
					(field id="x"
						(reference id="int" kind="type" line=0)
					)
					(field id="y"
						(reference id="float" kind="type" line=0)
					)
				)
			)
		)
	)
	(def_type id="f"
		(typedesc record
			(field id="x"
				(reference id="a" kind="type" line=1)
			)
			(field id="y"
				(reference id="b" kind="type" line=2)
			)
		)
	)
)
