(program
	(def_function id="norm"
		(reference id="int" kind="type" line=0)
		(parameter id="a"
			(reference id="int" kind="type" line=0)
		)
		(block
			(statement if
				(condition ==
					(expression variable
						(variable id
							(reference id="a" kind="parameter" line=1)
						)
					)
					(expression integer "0")
				)
				(block
					(statement return
						(expression integer "0")
					)
				)
				(block
					(statement if
						(condition >
							(expression variable
								(variable id
									(reference id="a" kind="parameter" line=1)
								)
							)
							(expression integer "0")
						)
						(block
							(statement return
								(expression integer "1")
							)
						)
						(block
							(statement return
								(expression -
									(expression integer "1")
								)
							)
						)
					)
				)
			)
		)
	)
)
