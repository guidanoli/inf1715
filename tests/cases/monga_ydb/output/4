(program
	(def_function id="norm" type="int"
		(parameter id="a" type="int")
		(block
			(statement if
				(condition ==
					(expression variable
						(variable "a")
					)
					(expression integer "0")
				)
				(block
					(statement return
						(expression integer "0")
					)
				)
				(block
					(statement if
						(condition >
							(expression variable
								(variable "a")
							)
							(expression integer "0")
						)
						(block
							(statement return
								(expression integer "1")
							)
						)
						(block
							(statement return
								(expression -
									(expression integer "1")
								)
							)
						)
					)
				)
			)
		)
	)
)
