(program
	(def_function id="ap"
		(reference id="int" kind="type" line=0)
		(parameter id="n"
			(reference id="int" kind="type" line=0)
		)
		(block
			(def_variable id="sum"
				(reference id="int" kind="type" line=0)
			)
			(statement =
				(variable id
					(reference id="sum" kind="variable" line=2)
				)
				(expression integer "0")
			)
			(statement while
				(condition >
					(expression variable
						(variable id
							(reference id="n" kind="parameter" line=1)
						)
					)
					(expression integer "0")
				)
				(block
					(statement =
						(variable id
							(reference id="sum" kind="variable" line=2)
						)
						(expression +
							(expression variable
								(variable id
									(reference id="sum" kind="variable" line=2)
								)
							)
							(expression variable
								(variable id
									(reference id="n" kind="parameter" line=1)
								)
							)
						)
					)
					(statement =
						(variable id
							(reference id="n" kind="parameter" line=1)
						)
						(expression -
							(expression variable
								(variable id
									(reference id="n" kind="parameter" line=1)
								)
							)
							(expression integer "1")
						)
					)
				)
			)
			(statement return
				(expression variable
					(variable id
						(reference id="sum" kind="variable" line=2)
					)
				)
			)
		)
	)
)
