(program
	(def_function id="f"
		(def_variable id="x"
			(reference id="int" kind="type" line=0)
		)
		(def_variable id="y"
			(reference id="float" kind="type" line=0)
		)
		(block
			(statement =
				(variable id
					(reference id="y" kind="variable" line=1)
				)
				(expression cast
					(reference id="float" kind="type" line=0)
					(expression variable
						(variable id
							(reference id="x" kind="variable" line=1)
						)
					)
				)
			)
		)
	)
)
