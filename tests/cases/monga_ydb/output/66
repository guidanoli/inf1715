(program
	(def_type id="x"
		(typedesc array
			(typedesc id
				(reference id="int" kind="type" line=0)
			)
		)
	)
	(def_function id="f"
		(parameter id="a"
			(reference id="x" kind="type" line=1)
		)
		(block
			(statement =
				(variable id
					(reference id="a" kind="parameter" line=2)
				)
				(expression null)
			)
		)
	)
)
