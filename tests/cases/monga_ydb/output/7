(program
	(def_type id="inta"
		(typedesc array
			(typedesc id
				(reference id="int" kind="type" line=0)
			)
		)
	)
	(def_type id="ivec"
		(typedesc record
			(field id="arr"
				(reference id="inta" kind="type" line=1)
			)
			(field id="size"
				(reference id="int" kind="type" line=0)
			)
		)
	)
	(def_function id="ivec_new"
		(reference id="ivec" kind="type" line=2)
		(parameter id="n"
			(reference id="int" kind="type" line=0)
		)
		(block
			(def_variable id="v"
				(reference id="ivec" kind="type" line=2)
			)
			(statement =
				(variable id
					(reference id="v" kind="variable" line=8)
				)
				(expression new
					(reference id="ivec" kind="type" line=2)
				)
			)
			(statement =
				(variable record
					(reference id="size" kind="field" line=4)
					(expression variable
						(variable id
							(reference id="v" kind="variable" line=8)
						)
					)
				)
				(expression variable
					(variable id
						(reference id="n" kind="parameter" line=7)
					)
				)
			)
			(statement =
				(variable record
					(reference id="arr" kind="field" line=3)
					(expression variable
						(variable id
							(reference id="v" kind="variable" line=8)
						)
					)
				)
				(expression new
					(reference id="int" kind="type" line=0)
					(expression variable
						(variable id
							(reference id="n" kind="parameter" line=7)
						)
					)
				)
			)
			(statement return
				(expression variable
					(variable id
						(reference id="v" kind="variable" line=8)
					)
				)
			)
		)
	)
)
