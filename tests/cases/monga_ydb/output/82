(program
	(def_type id="x"
		(typedesc record
			(field id="v"
				(reference id="int" kind="type" line=0)
			)
		)
	)
	(def_function id="checknull"
		(parameter id="a"
			(reference id="x" kind="type" line=1)
		)
		(block
			(statement if
				(condition ==
					(expression variable
						(variable id
							(reference id="a" kind="parameter" line=2)
						)
					)
					(expression null)
				)
				(block)
			)
		)
	)
)
